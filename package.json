{
  "name": "@radenadri/void-ui",
  "version": "0.2.3",
  "description": "A React component library built on Base UI and Tailwind CSS",
  "author": "Adrian",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwindcss",
    "base-ui",
    "design-system",
    "void-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BRANDNEWSHVT/void-ui"
  },
  "homepage": "https://github.com/BRANDNEWSHVT/void-ui#readme",
  "bugs": {
    "url": "https://github.com/BRANDNEWSHVT/void-ui/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@phosphor-icons/react": "^2.1.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
